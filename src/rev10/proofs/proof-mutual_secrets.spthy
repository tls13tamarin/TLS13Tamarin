     In( <$C, 
          senc(<pk(~ltkC), c_signature, client_fin>,
               HKDFExpand1(<HKDF(<'0', es, 'extractedES', '256'>), 
                            'handshake_key_expansion', h(h(prev_messages)), '256'>))
