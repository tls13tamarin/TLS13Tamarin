rule (modulo E) S_1_PSK_Auth:
   [
   F_St_S_1a_init_PSK( ~ns, $C, ~nc, $pc, $S, ~ns, $ps, ss, es, ~psk_id,
                       messages, config_hash, 'authenticated'
   )
   ]
  --[
  S1_PSK_Auth( ~ns ), CommitPSK( $S, $C, 'server', <~psk_id, ss> ),
  Instance( ~ns, $S, 'server' )
  ]->
   [
   St_S_1_init( ~ns, $C, ~nc, $pc, $S, ~ns, $ps, ss, es, messages,
                config_hash, 'auth_recv'
   )
   ]

  /* has exactly the trivial AC variant */

