  RunningSecrets( $C, $S, 'client', <'g'^b^~a, 'g'^b^~a> ),
  CommitNonces( $C, $S, 'client', <~nc, ~ns> ),
  CommitSS( $C, $S, 'client', 'g'^b^~a ),
  CommitES( $C, $S, 'client', 'g'^b^~a ),
  Eq( verify(s_signature,
             <<<prev_messages, ~ns, $ps, 'g'^b>, $exts, 'g'^y, $cert_req, pk(~ltkS)>, 
              'server_cert_verify'>,
             pk(~ltkS)),
      true
  ),
  Eq( server_fin,
