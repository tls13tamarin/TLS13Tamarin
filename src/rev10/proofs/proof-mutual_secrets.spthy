                    case C_2_NoAuth
                    by contradiction /* from formulas */
                  next
                    case chmac
                    solve( ( #r. (RevLtk( $C ) @ #r)  ((last(#r)))  (#r < #a.1))  
                           ( #r. (RevLtk( $S ) @ #r)  ((last(#r)))  (#r < #a.1)) )
                      case case_1
                      by contradiction /* from formulas */
                    next
                      case case_2
                      by contradiction /* from formulas */
                    qed
                  qed
                qed
              qed
            qed
          qed
        next
          case case_3
          by contradiction /* from formulas */
        qed
      qed
    qed
  next
    case case_3
    by contradiction /* from formulas */
  next
    case case_4
    by contradiction /* from formulas */
  qed
qed

