   ),
   Out( <$C, 
         senc(<pk(~ltkC), 
               sign(<<prev_messages, pk(~ltkC)>, 'client_cert_verify'>, ~ltkC), 
               hmac(<
                     HKDFExpand(<HKDF(<'0', ss, 'extractedSS', '256'>), 
                                 'finished_secret', 
                                 h(h(<<prev_messages, pk(~ltkC)>, 
                                      sign(<<prev_messages, pk(~ltkC)>, 'client_cert_verify'>,
                                           ~ltkC)
                                     >)), 
