                     config_hash, 'no_auth'
   ),
   !Ltk( $C, ~ltkC )
   ]
  --[
  C1_KC_Auth( ~nc ), Instance( ~nc, $C, 'client' ),
  UseLtk( $C, ~ltkC ),
  SignData( $C, <<prev_messages, config_hash>, 'client_cert_verify'>
