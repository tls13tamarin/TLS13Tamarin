   !Pk( $S, pk(~ltkS) ),
   In( <$S, ~ns, $ps, 'g'^b, 
        senc(<<$exts, 'g'^y, $cert_req, pk(~ltkS), s_signature>, server_fin
             >,
             HKDFExpand2(<HKDF(<'0', 'g'^b^~a, 'extractedES', '256'>), 
                          'handshake_key_expansion', h(h(<prev_messages, ~ns, $ps, 'g'^b>)), 
