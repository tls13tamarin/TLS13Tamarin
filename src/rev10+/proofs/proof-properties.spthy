solve( CommitTranscript( actor, peer, 'server', transcript ) @ #i )
  case S_2_Auth
  solve( ( #r. (RevLtk( $C ) @ #r))  
         ( #r. (RevLtk( $S ) @ #r))  
         ( #j.
           (RunningNonces( $C, $S, 'client', <~nc, ~ns> ) @ #j)  #j < #i) )
