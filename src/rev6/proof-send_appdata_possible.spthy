   [
   AuthenticMessage( <$C, ~nc, ~sid, $pc, 'g'^~x> ), DHExp( ~nc, ~x ),
   F_St_C_1_init( ~nc, $C, ~nc, ~sid, $pc, ~x )
   ]

  /* has exactly the trivial AC variant */

