lemma dh_chal_dual [reuse]:
  all-traces
  "∀ tid tid2 actor actor2 g x y gx gy gxy #i #j #r.
    (((((DHChal( g, x, y, gx, gy, gxy ) @ #i) ∧
        (Instance( tid, actor, 'client' ) @ #i)) ∧
       (DHChal( g, x, y, gx, gy, gxy ) @ #j)) ∧
      (Instance( tid2, actor2, 'server' ) @ #j)) ∧
     (!KU( gxy ) @ #r)) ⇒
    ((∃ #p. (RevDHExp( tid, actor, x ) @ #p) ∧ (#p < #r)) ∨
     (∃ #q. (RevDHExp( tid2, actor2, y ) @ #q) ∧ (#q < #r)))"
/*
guarded formula characterizing all counter-examples:
"∃ tid tid2 actor actor2 g x y gx gy gxy #i #j #r.
  (DHChal( g, x, y, gx, gy, gxy ) @ #i) ∧
  (Instance( tid, actor, 'client' ) @ #i) ∧
  (DHChal( g, x, y, gx, gy, gxy ) @ #j) ∧
  (Instance( tid2, actor2, 'server' ) @ #j) ∧
  (!KU( gxy ) @ #r)
 ∧
  (∀ #p. (RevDHExp( tid, actor, x ) @ #p) ⇒ ¬(#p < #r)) ∧
  (∀ #q. (RevDHExp( tid2, actor2, y ) @ #q) ⇒ ¬(#q < #r))"
*/
simplify
solve( DHChal( g, x, y, gx, gy, gxy ) @ #i )
  case recv_server_hello
  solve( Instance( tid.1, actor, 'client' ) @ #i )
    case recv_server_hello
    solve( DHChal( $g, ~x, y, gx, gy, gxy ) @ #j )
      case recv_server_hello
      by solve( Instance( tid2, actor2, 'server' ) @ #j )
    next
      case recv_server_hello_psk_dhe
      by solve( Instance( tid2, actor2, 'server' ) @ #j )
    next
      case server_hello
      solve( Instance( tid2, actor2, 'server' ) @ #j )
        case server_hello
        solve( F_State_C1( ~tid, $C, $S, p_res_psk, p_messages, p_nc, p_ns, $g,
                           p_sg, p_hrr, ~x, p_y, $g^~x, p_gy, p_gxy, p_psk_ke_mode, p_psk_id, p_edi,
                           p_es, p_hs, p_ms, p_cats, p_sats, p_hs_keyc, p_hs_keys, p_auth_status,
                           p_ems, p_rms, p_cert_req
               ) ▶₀ #i )
          case client_hello
          solve( F_State_S1( ~tid.1, $S.1, $C.1, p_res_psk, p_messages, p_nc, p_ns,
                             $g, $g, p_hrr, p_x, p_y, $g^~x, p_gy, p_gxy, p_psk_ke_mode, p_psk_id,
                             p_edi, p_es, p_hs, p_ms, p_cats, p_sats, p_hs_keyc, p_hs_keys,
                             p_auth_status, p_ems, p_rms, p_cert_req
                 ) ▶₀ #j )
            case recv_client_hello_case_1
            solve( !KU( $g^(~x*~y) ) @ #r )
              case cexp
              solve( !KU( ~x ) @ #vk.44 )
                case Reveal_DHExp
                solve( DH( ~tid.2, $A, ~x ) @ #vr )
                  case client_hello
                  by contradiction /* from formulas */
                qed
              qed
            next
              case client_hello
              solve( !KU( ~y ) @ #vk.43 )
