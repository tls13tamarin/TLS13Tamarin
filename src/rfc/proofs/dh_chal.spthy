               Expand(p_ms, <'serverats', h(<p_messages, '20', verify_data>)>, '32'),
               p_hs_keyc, p_hs_keys, p_auth_status,
               Expand(p_ms, <'ems', h(<p_messages, '20', verify_data>)>, '32'), p_rms,
               p_cert_req
